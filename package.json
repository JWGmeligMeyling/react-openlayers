{
  "name": "react-openlayers",
  "version": "0.2.1",
  "description": "OpenLayer React Components",
  "license": "MIT",
  "main": "dist/react-openlayers.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --port 9001 --content-base app --config app/webpack.config --open",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:umd build:app",
    "build:umd": "cross-env NODE_ENV=production webpack",
    "build:app": "cross-env NODE_ENV=production webpack --config app/webpack.config",
    "upgrade": "npm-check-updates -a/--upgradeAll && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenhwkim/react-openlayers.git"
  },
  "author": "Allen Kim<allenhwkim@gmail.com>",
  "bugs": {
    "url": "https://github.com/allenhwkim/react-openlayers/issues"
  },
  "homepage": "https://github.com/allenhwkim/react-openlayers#readme",
  "keywords": [
    "openlayer",
    "typescript",
    "react",
    "map"
  ],
  "devDependencies": {
    "@types/react": "^16.3.1",
    "@types/react-dom": "^16.0.4",
    "@types/googlemaps": "^3.26.4",
    "awesome-typescript-loader": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.26.2",
    "dts-generator": "^2.1.0",
    "file-loader": "^1.1.11",
    "geo-coder": "^0.2.1",
    "google-maps": "^3.2.1",
    "html-loader": "^0.4.5",
    "node-sass": "^4.5.0",
    "npm-run-all": "^3.1.2",
    "ol": "^4.6.5",
    "ol-mapbox-style": "^2.11.2",
    "qwest": "^4.4.5",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router": "^3.0.2",
    "rimraf": "^2.6.0",
    "style-loader": "^0.13.2",
    "ts-loader": "^4.1.0",
    "typescript": "^2.8.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "peerDependencies": {
    "ol": "^4.6.5",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  }
}
