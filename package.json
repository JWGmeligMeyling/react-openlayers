{
    "name": "react-openlayers",
    "version": "0.2.6",
    "description": "OpenLayer React Components",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": ["dist", "src"],
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack-dev-server --port 9001 --content-base app --config app/webpack.config --open",
        "clean": "rimraf dist",
        "build": "npm-run-all --serial clean build:tsc copy:static",
        "build:tsc": "tsc",
        "build:umd": "cross-env NODE_ENV=production webpack",
        "build:app": "cross-env NODE_ENV=production webpack --config app/webpack.config",
        "types": "dts-generator --name react-openlayers --project . --out dist/index.d.ts",
        "copy:static": "cp src/map.css dist/map.css && cp src/custom/popup.css dist/custom/popup.css",
        "packy": "npm pack && mv react-openlayers-0.2.6.tgz /Users/keithmoss/Downloads/react-openlayers-0.2.6.tgz",
        "wml:add": "wml add ./ /Users/keithmoss/Documents/Work/GitHub/ealgis-yarn/frontend/node_modules/react-openlayers",
        "wml:start": "wml start",
        "upgrade": "npm-check-updates -a/--upgradeAll && npm i"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/allenhwkim/react-openlayers.git"
    },
    "author": "Allen Kim<allenhwkim@gmail.com>",
    "bugs": {
        "url": "https://github.com/allenhwkim/react-openlayers/issues"
    },
    "homepage": "https://github.com/allenhwkim/react-openlayers#readme",
    "keywords": ["openlayer", "typescript", "react", "map"],
    "devDependencies": {
        "@types/googlemaps": "^3.26.4",
        "@types/ol": "^4.6.1",
        "@types/react": "^16.3.11",
        "@types/react-dom": "^16.0.4",
        "awesome-typescript-loader": "^5.0.0",
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "cross-env": "^4.0.0",
        "css-loader": "^0.26.2",
        "dts-generator": "^2.1.0",
        "file-loader": "^1.1.11",
        "geo-coder": "^0.2.1",
        "google-maps": "^3.2.1",
        "html-loader": "^0.4.5",
        "node-sass": "^4.5.0",
        "npm-run-all": "^3.1.2",
        "ol": "^4.6.5",
        "ol-mapbox-style": "^2.11.2",
        "qwest": "^4.4.5",
        "react": "^16.3.0",
        "react-dom": "^16.3.0",
        "react-router": "^3.0.2",
        "rimraf": "^2.6.0",
        "style-loader": "^0.13.2",
        "ts-loader": "^4.1.0",
        "typescript": "^2.8.1",
        "webpack": "^4.4.1",
        "webpack-cli": "^2.0.13",
        "webpack-dev-server": "^3.1.1"
    },
    "peerDependencies": {
        "ol": "^4.6.5",
        "react": "^16.3.0",
        "react-dom": "^16.3.0"
    }
}
